func list_sum(list){
    var sum = 0;
    var i = 0;
    while i < list.size do {
        sum = sum + list.get(i);
        i = i + 1;
    }
    return sum;
}

var numbers = List();
numbers.add(10);
numbers.add(15);
numbers.add(3);

println numbers;

println list_sum(numbers);