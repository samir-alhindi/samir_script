

# Lexical scope !

var x = "I'm outside";

{
    var x = "I'm in the middle";
    print x;
    {
        var x = "Inside !";
        print x;
    }
}

print x;

# Another demonstration:

var a = "global a";
var b = "global b";
var c = "global c";
{
  var a = "outer a";
  var b = "outer b";
  {
    var a = "inner a";
    print a;
    print b;
    print c;
  }
  print a;
  print b;
  print c;
}
print a;
print b;
print c;