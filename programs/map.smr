

func map(list, function){
    var i = 0;
    var output = List();
    var result = 0;
    while i < list.size do {
        var current = list.get(i);
        result = function(current);
        output.add(result);
        i = i + 1;
    }

    return output;
}

func sqr(num){
    return num * num;
}

var numbers = List();
numbers.add(1); numbers.add(2); numbers.add(3);
print map(numbers, sqr);