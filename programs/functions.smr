

func greeting(name, age){
    println "Hello " + name + " you are " + str(age) +" years old.";
}

func count(n){
    var i = 1;
    while i <= n do{
        println i;
        i = i + 1;
    }
    println "Done.";
}

func printSum(a, b, c){
    println a + b + c;
}

greeting("Samir", 20);
greeting("Yahya", 19);
greeting("Khair", 20);

count(15);
count(5);

printSum(5, 6, 0);